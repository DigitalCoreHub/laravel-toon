{
    "name": "digitalcorehub/laravel-toon",
    "description": "A friendly JSON transformer for Laravel — Convert JSON ↔ TOON, an ultra-minimal, line-based data format.",
    "version": "0.6",
    "keywords": [
        "laravel",
        "json",
        "toon",
        "format",
        "converter",
        "data-format",
        "streaming",
        "performance",
        "minimal"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Batuhan Haymana",
            "email": "haymana.batuhan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0|^12.0",
        "phpstan/phpstan": "^2.1",
        "laravel/pint": "^1.27"
    },
    "autoload": {
        "psr-4": {
            "DigitalCoreHub\\Toon\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalCoreHub\\Toon\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCoreHub\\Toon\\ToonServiceProvider"
            ],
            "aliases": {
                "Toon": "DigitalCoreHub\\Toon\\Facades\\Toon"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
